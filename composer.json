{
    "name": "aleksandar.rusakov/simple-file-search",
    "license": "MIT",
    "description": "This package gives you the ability to search for files by their content.",
    "type": "library",
    "authors": [
        {
            "name": "Aleksandar Rusakov",
            "email": "aleksandar.rusakov@rewardgateway.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        },
        "optimize-autoloader": true,
        "apcu-autoloader": true
    },
    "autoload-dev": {
        "psr-4": {
            "PHPUnit\\": "tests/PHPUnit/src",
            "PHPUnit\\TestCase\\": "tests/PHPUnit/test_case"
        }
    },
    "require": {
        "php": "^7.3",
        "symfony/console": "^4.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0",
        "symfony/var-dumper": "^4.2"
    },
    "scripts": {
        "phpunit": "./vendor/bin/phpunit --colors=always"
    }
}
